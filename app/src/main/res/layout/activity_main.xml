<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.liucheng.administrator.doubicinamatickit.ui.MainActivity">
    <android.support.v4.view.ViewPager
        android:id="@+id/mainactivity_viewPager"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"></android.support.v4.view.ViewPager>
    <TextView
        android:id="@+id/mainactivity_line"
        android:background="#8a8a8a"
        android:layout_width="match_parent"
        android:layout_height="0.5dp" />
<include

    layout="@layout/include_main_button_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"></include>

</LinearLayout>
